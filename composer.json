{
    "name": "davide-casiraghi/php-responsive-random-quote",
    "description": "A package that shows responsive random quotes",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Casiraghi",
            "email": "davide.casiraghi@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "dimsav/laravel-translatable": "^9.4",
        "nesbot/carbon": "^2.17"
    },
    "autoload": {
        "psr-4": {
            "DavideCasiraghi\\PhpResponsiveRandomQuote\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavideCasiraghi\\PhpResponsiveRandomQuote\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavideCasiraghi\\PhpResponsiveRandomQuote\\PhpResponsiveRandomQuoteServiceProvider"
            ],
            "aliases": {
                "PhpResponsiveQuote": "DavideCasiraghi\\PhpResponsiveRandomQuote\\PhpResponsiveRandomQuote"
            }
        }
    }
}
